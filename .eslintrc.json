{
	"env": {
		"browser": true,
		"commonjs": true,
		"es2021": true
	},
	"extends": ["airbnb-base"],
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"rules": {
		"no-console": "off",
		"linebreak-style": "off",
		"indent": [2, "tab"],
		"no-tabs": ["error", { "allowIndentationTabs": true }]
	}
}
